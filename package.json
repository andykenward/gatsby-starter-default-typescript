{
  "name": "gatsby-starter-default-typescript",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Andy Kenward <hello@andykenward.com>",
  "dependencies": {
    "@babel/core": "7.6.0",
    "gatsby": "2.15.19",
    "gatsby-image": "2.2.20",
    "gatsby-plugin-manifest": "2.2.18",
    "gatsby-plugin-offline": "3.0.8",
    "gatsby-plugin-react-helmet": "3.1.8",
    "gatsby-plugin-sharp": "2.2.25",
    "gatsby-plugin-typescript": "2.1.9",
    "gatsby-source-filesystem": "2.1.25",
    "gatsby-transformer-sharp": "2.2.16",
    "graphql": "14.5.7",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "ts-optchain": "0.1.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.10",
    "prettier": "1.18.2",
    "typescript": "3.6.3"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run type-check",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "postinstall": "gatsby clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andykenward/gatsby-starter-default-typescript"
  },
  "bugs": {
    "url": "https://github.com/andykenward/gatsby-starter-default-typescript/issues"
  }
}
